{
  "name": "@modusoperandi/licit-paste-json",
  "version": "0.0.12",
  "description": "MO's ProseMirror Plugin that allows you to paste in prosemirror JSON into the editor.",
  "main": "dist/index.js",
  "typings": "./index.d.ts",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MO-Movia/licit-plugin-contrib-paste-json.git"
  },
  "ngPackage": {
    "dest": "/dist/libs/"
  },
  "dependencies": {
    "prosemirror-model": "~1.16.0",
    "prosemirror-state": "1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.5",
    "@babel/plugin-proposal-do-expressions": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-sent": "^7.18.6",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/plugin-proposal-pipeline-operator": "^7.18.9",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-parameters": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/plugin-transform-typescript": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@cyclonedx/cyclonedx-npm": "^1.11.0",
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "babel-loader": "^9.1.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.29.0",
    "eslint-plugin-jest": "^27.1.7",
    "eslint-plugin-react": "^7.31.11",
    "file-loader": "^6.2.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-junit": "^15.0.0",
    "jest-prosemirror": "^1.0.10",
    "lint-staged": "^13.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "scripts": {
    "test": "jest --coverage",
    "clean": "rm -rf dist/ && rm -f modusoperandi-licit-paste-json-*.*.*.tgz",
    "webpack": "webpack",
    "build:ts": "tsc -p tsconfig.json",
    "build:babel": "babel src --out-dir dist --extensions .ts",
    "build:dist": "npm run clean && npm run build:ts && npm run webpack && npm run build:babel",
    "lint:ts": "eslint src --ext .ts --fix",
    "lint": "npm run lint:ts",
    "ci:build": "npm run clean && npm run build:ts && npm run webpack && npm run build:babel",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "prepare": "npm run build:dist",
    "publish:dist": "npm publish",
    "ci:bom": "cyclonedx-npm --ignore-npm-errors --short-PURLs --output-format XML --output-file dist/bom.xml"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module-grouping"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(*test|*.setup).ts": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
