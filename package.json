{
  "name": "@modusoperandi/licit-paste-json",
  "version": "0.0.3",
  "description": "MO's ProseMirror Plugin that allows you to paste in prosemirror JSON into the editor.",
  "main": "dist/index.js",
  "typings": "./index.d.ts",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MO-Movia/licit-plugin-contrib-paste-json.git"
  },
  "maintainers": [
    {
      "name": "Firmusoft Solutions"
    }
  ],
  "ngPackage": {
    "dest": "/dist/libs/"
  },
  "dependencies": {
    "@types/prosemirror-state": "^1.3.0",
    "prosemirror-model": "^1.18.1",
    "prosemirror-state": "^1.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.9",
    "@babel/plugin-proposal-do-expressions": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-function-sent": "^7.18.6",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-proposal-pipeline-operator": "^7.18.9",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-parameters": "^7.18.8",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/plugin-transform-typescript": "^7.18.8",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.20.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-react": "^7.30.1",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "jest": "^27.0.6",
    "jest-prosemirror": "^1.0.4",
    "lint-staged": "^13.0.3",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "scripts": {
    "test": "jest --coverage",
    "clean": "rm -rf dist/ && rm -f modusoperandi-licit-paste-json-*.*.*.tgz",
    "webpack": "webpack",
    "build:ts": "tsc -p tsconfig.json",
    "build:babel": "babel src --out-dir dist --extensions .ts",
    "build:dist": "npm run clean && npm run build:ts && npm run webpack && npm run build:babel",
    "lint:ts": "eslint src --ext .ts --fix",
    "lint:css": "echo no CSS available && exit 1",
    "prepare": "npm run build:dist",
    "publish:dist": "npm publish"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module-grouping"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(*test|*.setup).ts": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
